{
  "name": "dcc",
  "version": "0.1.27",
  "author": "Marece Williams",
  "description": "React and Electron Awesome App",
  "homepage": "./",
  "main": "public/main.js",
  "private": true,
  "icon": "icon.ico",
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.1.3",
    "electron-updater": "^4.6.1",
    "rc-slider": "^9.7.4",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "serialport": "^9.2.7"
  },
  "scripts": {
    "dev": "nf start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "install": "electron-rebuild",
    "rebuild": "electron-rebuild -f -w serialport",
    "package": "electron-builder build -c.extraMetadata.main=build/main.js --publish never",
    "deploy": "electron-builder build -c.extraMetadata.main=build/main.js --publish always",
    "backup": "git add . && git commit -m %NPM_PACKAGE_VERSION% && git push origin master",
    "upload": "yarn build && git add . && git commit -m %NPM_PACKAGE_VERSION% && git push origin master && yarn deploy ",
    "try": "yarn build && yarn package ",
    "refresh": "yarn upgrade --latest && yarn rebuild"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "electron-rebuild": "^3.2.5",
    "foreman": "^3.0.1"
  },
  "build": {
    "productName": "Big D's Railroad",
    "appId": "com.wbm.dcc",
    "electronVersion": "15.1.2",
    "copyright": "WBM Tek",
    "extends": null,
    "npmRebuild": false,
    "files": [
      "build/**/*",
      "electron/*"
    ],
    "extraResources": [
      "public/locos/*"
    ],
    "win": {
      "icon": "public/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wbmmusic/dcc.git"
  }
}
